\begin_layout Standard
\begin_inset Formula \begin{eqnarray*}
\frac{ad+bc}{bd} & = & \left(ad+bc\right)\cdot\left(bd\right)^{-1}\\
 & = & \left(ad+bc\right)\cdot b^{-1}\cdot d^{-1}\\
 & = & ad\cdot b^{-1}\cdot d^{-1}+bc\cdot b^{-1}\cdot d^{-1}\\
 & = & ad\cdot b^{-1}\cdot d^{-1}+bc\cdot b^{-1}\cdot d^{-1}\\
 & = & a\cdot b^{-1}\cdot d\cdot d^{-1}+c\cdot b\cdot b^{-1}\cdot d^{-1}\\
 & = & a\cdot b^{-1}\cdot1+c\cdot1\cdot d^{-1}\\
 & = & a\cdot b^{-1}+c\cdot d^{-1}\\
 & = & \frac{a}{b}+\frac{c}{d}\end{eqnarray*}

