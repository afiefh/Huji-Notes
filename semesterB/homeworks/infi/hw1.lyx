\begin_layout Subsubsection
צ"ל כי 
\begin_inset Formula $x^{3}-y^{3}=\left(x-y\right)\left(x^{2}+xy+y^{2}\right)$
\end_inset


\end_layout

\begin_layout Standard
נשתמש בחוק הפילוג על צד שמאלי של המשוואה
\end_layout

\begin_layout Standard
\begin_inset Formula \begin{eqnarray*}
\left(x-y\right)\left(x^{2}+xy+y^{2}\right) & \overset{\mbox{\R{פילוג}}}{=} & x\cdot\left(x^{2}+xy+y^{2}\right)-y\cdot\left(x^{2}+xy+y^{2}\right)\\
 & \overset{\mbox{\R{פילוג}}}{=} & x\cdot x^{2}+x\cdot xy+x\cdot y^{2}-y\cdot x^{2}-y\cdot xy-y\cdot y^{2}\\
 & = & x^{3}+x^{2}y+x\cdot y^{2}-x^{2}y-xy^{2}-y^{3}\\
 & \overset{\mbox{\R{חילוף}}}{=} & x^{3}+x^{2}y-x^{2}y+x\cdot y^{2}-xy^{2}-y^{3}\\
 & \overset{\mbox{\R{קיבוץ}}}{=} & x^{3}+\left(x^{2}y-x^{2}y\right)+\left(x\cdot y^{2}-xy^{2}\right)-y^{3}\\
 & \overset{\mbox{\R{הפיך\,חיבורי}}}{=} & x^{3}+0+0-y^{3}\\
 & \overset{\mbox{\R{נוטרלי\,לחיבור}}}{=} & x^{3}-y^{3}\end{eqnarray*}

\end_inset


