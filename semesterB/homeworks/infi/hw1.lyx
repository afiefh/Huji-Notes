\begin_layout Standard
\begin_inset Formula \begin{eqnarray*}
\frac{a\cdot c}{b\cdot c} & = & a\cdot c\left(b\cdot c\right)^{-1}\\
 & = & a\cdot c\cdot b^{-1}\cdot c^{-1}\\
 & = & a\cdot b^{-1}c\cdot c^{-1}\\
 & = & a\cdot b^{-1}\cdot1\\
 & = & a\cdot b^{-1}\\
 & = & \frac{a}{b}\end{eqnarray*}

\end_inset


\end_layout

