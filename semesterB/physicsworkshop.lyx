\end_inset

.
 צפיפות המטען היא 
\begin_inset Formula $\sigma=\frac{q}{A}$
\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula \begin{eqnarray*}
u & = & \frac{1}{8\pi k}\int E^{2}dv\\
 & = & \frac{1}{8\pi k}\left(4\pi k\frac{q}{A}\right)^{2}Ad\\
 & = & \frac{2\pi kq^{2}d}{A}\\
 & = & \frac{q^{2}d}{2\varepsilon_{0}A}\\
 & = & \frac{q^{2}}{2c}\\
 & \underbrace{=}_{q=cv} & \frac{1}{2}cv^{2}\end{eqnarray*}

\end_inset


\end_layout

