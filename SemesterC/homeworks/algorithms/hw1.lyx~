\begin_layout Standard
נתון כי 
\begin_inset Formula $f\left(n\right)=\Theta\left(g\left(n\right)\right)$
\end_inset

 כלומר קיים 
\begin_inset Formula $c_{1}$
\end_inset

 כך שעבור 
\begin_inset Formula $n>n_{1}$
\end_inset

 מתקיים 
\begin_inset Formula $f\left(n\right)<c_{1}\cdot g\left(n\right)$
\end_inset

 וגם קיים 
\begin_inset Formula $c_{n}$
\end_inset

 כך שעבור 
\begin_inset Formula $n>n_{2}$
\end_inset

 מתקיים 
\begin_inset Formula $f\left(n\right)>c_{2}\cdot g\left(n\right)$
\end_inset


\end_layout

\begin_layout Subsection
