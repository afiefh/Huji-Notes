\end_inset


\end_layout

\begin_layout Subsubsection
\begin_inset Formula $\log\left(f\left(n\right)\right)=\mathcal{O}\left(\log\left(g\left(n\right)\right)\right)$
\end_inset


\end_layout

\begin_layout Standard
צריך למצא 
\begin_inset Formula $c$
\end_inset

 ו 
\begin_inset Formula $n_{0}$
\end_inset

 כך ש עבור 
\begin_inset Formula $n>n_{0}$
\end_inset

 מתקיים 
\begin_inset Formula $\log\left(f\left(n\right)\right)<c\log\left(g\left(n\right)\right)$
\end_inset

.
 יודעים כי עבור 
\begin_inset Formula $n_{1},c_{1}$
\end_inset

 מתקיים 
\begin_inset Formula $f\left(n\right)<c_{1}\cdot g\left(n\right)$
\end_inset

 אזי
\end_layout

\begin_layout Standard
\begin_inset Formula \begin{eqnarray*}
\log\left(f\left(n\right)\right) & < & \log\left(g\left(n\right)\right)<c\log\left(c_{1}\cdot f\left(n\right)\right)\\
\log\left(f\left(n\right)\right) & < & c\log\left(c_{1}f\left(n\right)\right)\\
\log\left(f\left(n\right)\right) & < & c\log\left(c_{1}\right)+c\log\left(f\left(n\right)\right)\\
0 & < & c\log\left(c_{1}\right)+\left(c-1\right)\log\left(f\left(n\right)\right)\\
k & < & c^{\prime}\log\left(f\left(n\right)\right)\\
\frac{k}{\log\left(f\left(n\right)\right)} & < & c^{\prime}\end{eqnarray*}

\end_inset


\end_layout

\begin_layout Standard
בוחרים את 
\begin_inset Formula $n$
\end_inset

 כך ש 
\begin_inset Formula $f\left(n\right)$
\end_inset

 מינימאלי בתחום 
\begin_inset Formula $\left(n_{1},\infty\right)$
\end_inset

 ואז אין בעיה בבחירה של 
\begin_inset Formula $c^{\prime}$
\end_inset

 כזה ואת לחשב את 
\begin_inset Formula $c$
\end_inset

.
