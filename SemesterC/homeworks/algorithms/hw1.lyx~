\end_layout

\begin_layout Standard
\begin_inset Formula \begin{eqnarray*}
\log\left(f\left(n\right)\right) & > & \log\left(g\left(n\right)\right)<c\log\left(c_{2}\cdot f\left(n\right)\right)\\
\log\left(f\left(n\right)\right) & > & c\log\left(c_{2}f\left(n\right)\right)\\
\log\left(f\left(n\right)\right) & > & c\log\left(c_{2}\right)+c\log\left(f\left(n\right)\right)\\
0 & > & c\log\left(c_{2}\right)+\left(c-1\right)\log\left(f\left(n\right)\right)\\
k & > & c^{\prime}\log\left(f\left(n\right)\right)\\
\frac{k}{\log\left(f\left(n\right)\right)} & > & c^{\prime}\end{eqnarray*}

\end_inset


