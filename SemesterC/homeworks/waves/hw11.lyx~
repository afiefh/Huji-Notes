\begin_layout Standard
\begin_inset Formula \begin{eqnarray*}
M_{lense} & = & \begin{pmatrix}1 & 0\\
-\frac{1}{f} & 1\end{pmatrix}\\
D & = & M_{v}\cdot M_{lense}\cdot M_{u}\\
 & = & \begin{pmatrix}1-\frac{V}{f} & \overbrace{v+u+\frac{uv}{f}}^{0}\\
-\frac{1}{f} & 1-\frac{u}{f}\end{pmatrix}\\
 & = & \begin{pmatrix}-\frac{2}{3} & 0\\
-2.5 & -\frac{3}{2}\end{pmatrix}\\
\begin{pmatrix}r\\
\alpha\end{pmatrix}_{2} & = & D\begin{pmatrix}r\\
\alpha\end{pmatrix}\\
r_{2} & = & r_{1}\left(-\frac{2}{3}\right)\\
\alpha_{2} & = & r_{1}\left(-\frac{5}{2}\right)+\left(-\frac{3}{2}\right)\alpha_{1}\end{eqnarray*}

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula \[
\frac{r_{2}}{r_{1}}=-\frac{2}{3}\]

\end_inset


\end_layout

