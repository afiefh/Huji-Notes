\begin_layout Standard
\begin_inset Formula \begin{eqnarray*}
\frac{1}{Z\left(j\omega\right)} & = & \frac{1}{R}+j\omega C+\frac{1}{j\omega L}\\
 & = & \frac{j\omega L-\omega^{2}RCL+R}{j\omega LR}\\
Z\left(j\omega\right) & = & \frac{j\omega LR}{j\omega L-\omega^{2}RCL+R}\\
 & = & \frac{8\cdot3\cdot4j}{j8\cdot3-8^{2}\cdot4\cdot3\cdot52.1\cdot10^{-3}+4}\\
 & = & \frac{96j}{24j-40+4}\\
 & = & \frac{-24j}{9-6j}\cdot\frac{\left(9+6j\right)}{\left(9+6j\right)}\\
 & = & \frac{144-216j}{9^{2}+6^{2}}=1.23-1.84j\end{eqnarray*}

\end_inset


