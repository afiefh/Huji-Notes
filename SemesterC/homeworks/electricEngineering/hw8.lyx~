\begin_layout Standard
\begin_inset Formula \begin{eqnarray*}
Z_{ab} & = & \left(1+2j\omega\right)\parallel\frac{1}{2j\omega}\parallel2n^{2}\\
 & = & \left(1+2j\omega\right)\parallel\left(\frac{\frac{2n^{2}}{2j\omega}}{2n^{2}+\frac{i}{2\omega}}\right)\\
 & = & \left(1+2j\omega\right)\parallel\left(\frac{n^{2}}{2n^{2}j\omega+\frac{1}{2}}\right)\\
 & = & \left(1+2j\omega\right)\parallel\left(\frac{2n^{2}}{4n^{2}j\omega+1}\right)\\
 & = & \frac{\left(\frac{\left(1+2j\omega\right)2n^{2}}{4n^{2}j\omega+1}\right)}{\left(1+2j\omega\right)+\frac{2n^{2}}{4n^{2}j\omega+1}}\\
 & = & \frac{\left(1+2j\omega\right)2n^{2}}{\left(1+2j\omega\right)\left(1+4n^{2}j\omega\right)+2n^{2}}\\
 & = & \frac{2n^{2}+4n2\omega j}{1+4n^{2}j\omega+2j\omega-8n^{2}\omega^{2}+2n^{2}}\end{eqnarray*}

