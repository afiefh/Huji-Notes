\begin_layout Standard
דרושים לנו ת"ה ב 
\begin_inset Formula $v_{c}\left(0^{-}\right)$
\end_inset

 וב 
\begin_inset Formula $v_{c}^{\prime}\left(0^{-}\right)$
\end_inset

.
 נמיר את ת"ה ב 
\begin_inset Formula $i_{L}\left(0^{-}\right)$
\end_inset

 בת"ה ב 
\begin_inset Formula $v_{c}^{\prime}\left(0^{-}\right)$
\end_inset

:
\end_layout

\begin_layout Standard
\begin_inset Formula \begin{eqnarray*}
i_{C}\left(0^{-}\right) & = & -i_{R}\left(0^{-}\right)-i_{L}\left(0^{-}\right)=-\frac{V_{0}}{R}-I_{0}\\
\Rightarrow Cv_{c}^{\prime}\left(0^{-}\right) & = & -\frac{V_{0}}{R}-I_{0}\\
V_{c}^{\prime}\left(0^{-}\right) & = & -\frac{V_{0}}{RC}-\frac{I_{0}}{C}=-I_{0}-20V_{0}\end{eqnarray*}

\end_inset


\end_layout

\begin_layout Standard
נציב ת"ה:
\end_layout

\begin_layout Standard
\begin_inset Formula \begin{eqnarray*}
v_{c}^{zir}\left(0\right) & = & A=v_{c}\left(0^{-}\right)\\
v_{c}^{zir}\left(0\right) & = & A=V_{0}\\
v_{c}^{\prime^{zir}}\left(0\right) & = & -10Ae^{-10t}+Be^{-10t}-10Bte^{-10t}\\
v_{c}^{\prime^{zir}}\left(0\right) & = & -10V_{0}e^{-10t}+B=-I_{0}-20V_{0}\\
\Rightarrow B & = & I_{0}-10V_{0}\\
\Rightarrow v_{c}^{zir}\left(t\right) & = & V_{0}e^{-10t}-\left(I_{0}+10V_{0}\right)te^{-10t};t\geq0\end{eqnarray*}

\end_inset


\end_layout

