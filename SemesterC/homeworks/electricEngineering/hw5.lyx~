\begin_layout Standard
לפי כרמר:
\end_layout

\begin_layout Standard
\begin_inset Formula \begin{eqnarray*}
V_{c} & = & V_{2}\\
V_{c} & = & \frac{\frac{V_{s}}{R_{2}sL}}{\left(\frac{1}{R_{2}}+\frac{1}{sL}\right)\left(\frac{1}{R_{1}}+\frac{1}{Ls}+Cs\right)-\frac{1}{s^{2}L}}\\
\frac{1}{Lc}V_{s} & = & V_{c}^{\prime\prime}+\left(\frac{1}{R_{1}C}+\frac{R_{2}}{L}\right)V_{c}^{\prime}+\frac{1}{LC}\left(1+\frac{R_{2}}{R_{1}}\right)V_{c}\\
2V_{s} & = & V_{c}^{\prime\prime}+20.2V_{c}^{\prime}+6V_{c}\\
2u\left(t\right) & = & V_{c}^{\prime\prime}+20.2V_{c}^{\prime}+6V_{c}\\
\omega^{2} & = & 6\\
\alpha & = & 10.1\\
Q & = & 0.12\end{eqnarray*}

\end_inset


\end_layout

