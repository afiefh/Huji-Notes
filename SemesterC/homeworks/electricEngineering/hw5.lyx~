\begin_layout Subsection
א
\end_layout

\begin_layout Standard
\begin_inset Formula \begin{eqnarray*}
RI & = & V\\
\begin{pmatrix}R_{1}+Ls & -Ls\\
-Ls & R_{2}+\frac{1}{Cs}+Ls\end{pmatrix}\begin{pmatrix}I_{1}\\
I_{2}\end{pmatrix} & = & \begin{pmatrix}V_{s}\left(s\right)\\
0\end{pmatrix}\\
V_{c} & = & \frac{1}{Cs}I_{2}\end{eqnarray*}

\end_inset


\end_layout

\begin_layout Standard
נשתמש בכלל כרמר ומקבלים:
\end_layout

\begin_layout Standard
\begin_inset Formula \begin{eqnarray*}
I_{1} & = & \frac{\begin{vmatrix}V_{s}\left(s\right) & -Ls\\
0 & R_{2}+\frac{1}{Cs}+Ls\end{vmatrix}}{\begin{vmatrix}R_{1}+Ls & -Ls\\
-Ls & R_{2}+\frac{1}{Cs}+Ls\end{vmatrix}}\\
 & = & \frac{\left(R_{2}+\frac{1}{Cs}+Ls\right)V_{s}\left(s\right)}{\left({\it R_{1}}+sL\right)\left({\it R_{2}}+sL+\frac{1}{sC}\right)-s^{2}L^{2}}\end{eqnarray*}

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula \begin{eqnarray*}
I_{2} & = & \frac{\begin{vmatrix}R_{1}+Ls & V_{s}\left(s\right)\\
-Ls & 0\end{vmatrix}}{\begin{vmatrix}R_{1}+Ls & -Ls\\
-Ls & R_{2}+\frac{1}{Cs}+Ls\end{vmatrix}}\\
 & = & \frac{Ls\cdot V_{s}\left(s\right)}{\left({\it R_{1}}+sL\right)\left({\it R_{2}}+sL+\frac{1}{sC}\right)-s^{2}L^{2}}\end{eqnarray*}

\end_inset


\end_layout

\begin_layout Standard
מציבים:
\end_layout

\begin_layout Standard
\begin_inset Formula \begin{eqnarray*}
V_{C} & = & \frac{1}{Cs}\cdot\frac{Ls\cdot V_{s}\left(s\right)}{\left({\it R_{1}}+sL\right)\left({\it R_{2}}+sL+\frac{1}{sC}\right)-s^{2}L^{2}}\\
V_{c} & = & \frac{L\cdot V_{s}\left(s\right)}{C\left({\it R_{1}}+sL\right)\left({\it R_{2}}+sL+\frac{1}{sC}\right)-s^{2}L^{2}C}\\
V_{c} & = & \frac{sL\cdot V_{s}\left(s\right)}{\left(R_{1}R_{2}Cs+R_{1}s^{2}LC+R_{1}sC\frac{1}{sC}+s^{2}LCR_{2}+s^{3}L^{2}C+\frac{s^{2}LC}{sC}\right)-s^{3}L^{2}C}\\
V_{c} & = & \frac{sL\cdot V_{s}\left(s\right)}{R_{1}+s\left(R_{1}R_{2}C+L\right)+s^{2}\left(R_{1}LC+LCR_{2}\right)}\\
sL\cdot V_{s}\left(s\right) & = & V_{c}\left[R_{1}+s\left(R_{1}R_{2}C+L\right)+s^{2}\left(R_{1}LC+LCR_{2}\right)\right]\\
LV_{s}^{\prime}\left(s\right) & = & V_{c}R_{1}+V_{c}^{\prime}\left(R_{1}R_{2}C+L\right)+V_{c}^{\prime\prime}\left(R_{1}LC+LCR_{2}\right)\\
\overbrace{\frac{L}{\left(R_{1}LC+LCR_{2}\right)}}^{K}V_{s}^{\prime} & = & V_{c}^{\prime\prime}+V_{c}^{\prime}\overbrace{\frac{\left(R_{1}R_{2}C+L\right)}{\left(R_{1}LC+LCR_{2}\right)}}^{2\alpha}+V_{c}\overbrace{\frac{R_{1}}{\left(R_{1}LC+LCR_{2}\right)}}^{\omega_{0}^{2}}\end{eqnarray*}

\end_inset


\end_layout

\begin_layout Standard
אז:
\end_layout

\begin_layout Standard
\begin_inset Formula \begin{eqnarray*}
\omega_{0} & = & \sqrt{\frac{R_{1}}{\left(R_{1}LC+LCR_{2}\right)}}\\
Q & = & \frac{\omega_{0}}{2\alpha}\\
 & = & \frac{\sqrt{R_{1}}}{\left(\frac{\left(R_{1}R_{2}C+L\right)}{\sqrt{\left(R_{1}LC+LCR_{2}\right)}}\right)}\\
 & = & \frac{\sqrt{R_{1}}\sqrt{\left(R_{1}LC+LCR_{2}\right)}}{\left(R_{1}R_{2}C+L\right)}\end{eqnarray*}

\end_inset


\end_layout

\begin_layout Subsection
ב
\end_layout

\begin_layout Standard
\begin_inset Formula \begin{eqnarray*}
\omega_{0} & = & \sqrt{\frac{R_{1}}{\left(R_{1}LC+LCR_{2}\right)}}\\
 & = & \sqrt{\frac{2}{2\cdot4\cdot8+4\cdot8\cdot1}}\\
 & = & \sqrt{\frac{2}{64+48}}\\
 & = & \sqrt{\frac{2}{96}}\\
 & = & \frac{1}{\sqrt{48}}\end{eqnarray*}

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula \begin{eqnarray*}
\alpha & = & \left(\frac{5}{48}\right)\\
Q & = & \frac{\omega}{2\alpha}\\
 & = & \frac{\left(\frac{1}{\sqrt{48}}\right)}{\left(\frac{5}{48}\right)}=\frac{1}{5\sqrt{48}}\\
K & = & \frac{L}{\left(R_{1}LC+LCR_{2}\right)}\\
 & = & \frac{4}{96}=\frac{1}{24}\end{eqnarray*}

\end_inset


\end_layout

\begin_layout Standard
פתרון:
\end_layout

\begin_layout Standard
\begin_inset Formula \begin{eqnarray*}
PV_{c}^{\prime} & = & V_{c}^{\prime\prime}+2\alpha V_{c}^{\prime}+\omega^{2}V_{c}\end{eqnarray*}

\end_inset


\end_layout

\begin_layout Standard
פתרון הומוגיני:
\end_layout

\begin_layout Standard
\begin_inset Formula \begin{eqnarray*}
0 & = & V_{c}^{\prime\prime}+2\alpha V_{c}^{\prime}+\omega^{2}V_{c}\\
0 & = & V_{C}^{\prime\prime}+\frac{5}{24}V_{C}^{\prime}+\frac{1}{48}V_{C}\end{eqnarray*}

\end_inset


\end_layout

\begin_layout Standard
נפתור פולינום אופייני:
\end_layout

\begin_layout Standard
\begin_inset Formula \begin{eqnarray*}
s^{2}+\frac{5}{24}s+\frac{1}{48} & = & 0\\
s_{1} & = & \frac{-5+i\sqrt{23}}{48}\\
s_{2} & = & \frac{-5-i\sqrt{23}}{48}\\
V_{c}\left(t\right) & = & Ae^{s_{1}t}+Be^{s_{2}t}\end{eqnarray*}

\end_inset


\end_layout

\begin_layout Standard
פתרון הומוגני:
\end_layout

\begin_layout Standard
\begin_inset Formula \begin{eqnarray*}
V_{c} & = & -\overbrace{\frac{24}{\sqrt{23}}}^{p}ie^{\frac{-5+i\sqrt{23}}{48}t}+\frac{24i}{\sqrt{23}}e^{\frac{-5-i\sqrt{23}}{48}t}\\
 & = & -pie^{\frac{-5}{48}t}\left(i\sin\left(\frac{\sqrt{23}}{48}t\right)+\cos\left(\frac{\sqrt{23}}{48}t\right)\right)+pie^{\frac{-5}{48}t}\left(i\sin\left(-\frac{\sqrt{23}}{48}t\right)+\cos\left(-\frac{\sqrt{23}}{48}t\right)\right)\\
 & = & pe^{\frac{-5}{48}t}\left(\sin\left(\frac{\sqrt{23}}{48}t\right)-i\cos\left(\frac{\sqrt{23}}{48}t\right)\right)+pie^{\frac{-5}{48}t}\left(\sin\left(\frac{\sqrt{23}}{48}t\right)+i\cos\left(\frac{\sqrt{23}}{48}t\right)\right)\\
 & = & \frac{48}{\sqrt{23}}e^{\frac{-5}{48}t}\sin\left(\frac{\sqrt{23}}{48}t\right)\end{eqnarray*}

\end_inset


\end_layout

\begin_layout Standard
פותרים 
\begin_inset Formula $ZSR$
\end_inset

:
\end_layout

\begin_layout Standard
\begin_inset Formula \[
V_{C}^{\prime\prime}+\frac{5}{24}V_{C}^{\prime}+\frac{1}{48}V_{C}=\frac{1}{24}\cdot2\cdot u\left(t\right)\]

\end_inset

 מנחשים פתרון: 
\begin_inset Formula $V_{p}=a+bt$
\end_inset

 ומציבים
\end_layout

\begin_layout Standard
\begin_inset Formula \begin{eqnarray*}
V_{C}^{ZSR} & = & \left(V_{p}+V_{h}\right)u\left(t\right)\\
 & = & a+bt+Ae^{\frac{-5+i\sqrt{23}}{48}t}+Be^{\frac{-5-i\sqrt{23}}{48}t}\\
\frac{5}{24}b+\frac{1}{48}\left(a+bt\right) & = & \frac{1}{12}\\
\left(\frac{5}{24}b+\frac{1}{48}a\right)+\left(\frac{1}{48}b\right)t & = & \frac{1}{12}\\
b & = & 0\\
a & = & 4\end{eqnarray*}

\end_inset


\end_layout

\begin_layout Standard
מציבים בתנאי התחלה
\end_layout

\begin_layout Standard
\begin_inset Formula \begin{eqnarray*}
V_{C}^{ZSR} & = & 4+Ae^{\frac{-5+i\sqrt{23}}{48}t}+Be^{\frac{-5-i\sqrt{23}}{48}t}\\
0 & = & 4+A+B\\
0 & = & A\left(\frac{-5+i\sqrt{23}}{48}\right)+B\left(\frac{-5-i\sqrt{23}}{48}\right)\\
A & = & -2+\frac{10i}{\sqrt{23}}\\
B & = & -2-\frac{10i}{\sqrt{23}}\end{eqnarray*}

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula \[
V_{c}=\frac{4}{23}e^{-\frac{5t}{48}}\left(23e^{\frac{5t}{48}}-23\cos\left(\frac{\sqrt{23}t}{48}\right)-5\sqrt{23}\sin\left(\frac{\sqrt{23}t}{48}\right)\right)u\left(t\right)\]

