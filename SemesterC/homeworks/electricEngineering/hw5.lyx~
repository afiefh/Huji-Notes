\begin_layout Standard
נציב:
\end_layout

\begin_layout Standard
\begin_inset Formula \begin{eqnarray*}
i_{s}\left(t\right) & = & cv_{c}^{\prime}\left(t\right)+\frac{1}{R}v_{c}\left(t\right)+\frac{1}{L}\int_{0}^{t}v_{c}\left(t\right)dt\\
\frac{1}{C}i_{s}^{\prime}\left(t\right) & = & v_{c}^{\prime\prime}\left(t\right)+\frac{1}{RC}v_{c}^{\prime}\left(t\right)+\frac{1}{LC}v_{c}\left(t\right)\end{eqnarray*}

\end_inset


\end_layout

\begin_layout Standard
מציבים הנתונים:
\end_layout

\begin_layout Standard
\begin_inset Formula \begin{eqnarray*}
\omega_{0} & = & 10=\frac{1}{\sqrt{LC}}=\frac{10}{\sqrt{L}}\Rightarrow L=1H\\
Q & = & \frac{\omega_{0}}{2\alpha}=\frac{10}{2\alpha}=\frac{10}{\nicefrac{1}{RC}}=\frac{10}{R}=\frac{1}{2}\Rightarrow R=\frac{1}{20}\Omega\\
i_{s}^{\prime}\left(t\right) & = & v_{c}^{\prime\prime}\left(t\right)+20v_{c}^{\prime}\left(t\right)+100v_{C}\left(t\right)=i_{s}^{\prime}\left(t\right)\end{eqnarray*}

\end_inset


\end_layout

