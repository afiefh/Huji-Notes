\begin_layout Standard
\begin_inset Formula \begin{eqnarray*}
x\left(t\right) & = & \left(1-t\right)u\left(t\right)u\left(1-t\right)\\
h\left(t\right) & = & \left(1-t\right)u\left(t\right)u\left(1-t\right)\end{eqnarray*}

\end_inset


\end_layout

\begin_layout Standard
