\begin_layout Standard
\begin_inset Formula \begin{eqnarray*}
h\left(t\right) & = & e^{-\alpha t}u\left(t\right)\\
x\left(t\right) & = & \sin\left(\omega t\right)u\left(t\right)\end{eqnarray*}

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula \begin{eqnarray*}
\left(h*x\right)\left(t\right) & = & \int_{-\infty}^{t}h\left(\tau\right)\cdot x\left(t-\tau\right)d\tau\\
 & = & \begin{cases}
t<0 & 0\\
t>0 & \int_{-\infty}^{t}h\left(\tau\right)\cdot x\left(t-\tau\right)d\tau\end{cases}\\
 & = & \begin{cases}
t<0 & 0\\
t>0 & \int_{-\infty}^{t}e^{-a\tau}\cdot\sin\left(\omega\left(t-\tau\right)\right)d\tau\end{cases}\\
 & = & \begin{cases}
t<0 & 0\\
t>0 & \left[\frac{e^{-\alpha k}\left(\alpha\sin\left(\omega\left(\tau-t\right)\right)+\omega\cos\left(\tau-t\right)\right)}{\alpha^{2}+\omega^{2}}\right]_{0}^{t}\end{cases}\\
 &  & \begin{cases}
t<0 & 0\\
t>0 & \frac{\omega e^{-\alpha t}+\alpha\sin\left(\omega t\right)-\omega\cos\left(\omega t\right)}{\alpha^{2}+\omega^{2}}\end{cases}\end{eqnarray*}

\end_inset


\end_layout

\begin_layout Subsection
ב
\end_layout

\begin_layout Standard
\begin_inset Formula \begin{eqnarray*}
h\left(t\right) & = & 2\sin\left(\omega t\right)u\left(t\right)\\
x\left(t\right) & = & 2\cos\left(\omega t\right)u\left(t\right)\end{eqnarray*}

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula \begin{eqnarray*}
\left(h*x\right)\left(t\right) & = & \begin{cases}
t<0 & 0\\
t\geq0 & \int_{0}^{t}h\left(t-\tau\right)x\left(t\right)d\tau\end{cases}\\
 & = & \begin{cases}
t<0 & 0\\
t\geq0 & 4\int_{0}^{t}\sin\left(\omega\left(t-\tau\right)\right)\cos\left(\omega\tau\right)d\tau\end{cases}\\
 & = & \begin{cases}
t<0 & 0\\
t\geq0 & 4\cdot\frac{2\tau\omega\sin\left(\omega t\right)+\cos\left(\omega\left(2\tau-t\right)\right)}{4\omega}\end{cases}\\
 & = & \begin{cases}
t<0 & 0\\
t\geq0 & \frac{2\tau\omega\sin\left(\omega t\right)+\cos\left(\omega\left(2\tau-t\right)\right)}{\omega}\end{cases}\\
 & = & \begin{cases}
t<0 & 0\\
t\geq0 & 2t\sin\left(\omega t\right)\end{cases}\end{eqnarray*}

\end_inset


\end_layout

\begin_layout Subsection
ג
\end_layout

\begin_layout Standard
\begin_inset Formula \begin{eqnarray*}
h\left(t\right) & = & 8r\left(t\right)\\
x\left(t\right) & = & 3u\left(t\right)-2u\left(t-6\right)\end{eqnarray*}

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula \begin{eqnarray*}
\left(h*x\right)\left(t\right) & = & \begin{cases}
t<0 & 0\\
t\geq0 & h\left(t-\tau\right)x\left(\tau\right)\end{cases}\\
 & = & \begin{cases}
t<0 & 0\\
t\geq0 & \int_{0}^{t}\left(8r\left(t-\tau\right)\right)\left(3u\left(\tau\right)-2u\left(\tau-6\right)\right)d\tau\end{cases}\\
 & = & \begin{cases}
t<0 & 0\\
t\geq0 & \int_{0}^{t}\left(8r\left(t-\tau\right)\right)\left(3u\left(\tau\right)-2u\left(\tau-6\right)\right)d\tau\end{cases}\\
 & = & \begin{cases}
t<0 & 0\\
t\geq0 & \left[\left(\tau-6\right)u\left(\tau-6\right)\left(\tau-2t+6\right)-\frac{3}{2}\tau u\left(\tau\right)\left(\tau-2t\right)\right]\cdot8r\left(t\right)\end{cases}\\
 & = & \begin{cases}
t\geq0 & \begin{matrix}8u\left[-6+k\right]\left(36-k^{2}+\left(-36+t^{2}\right)u\left[6-t\right]+\left(k^{2}-t^{2}\right)u\left[k-t\right]\right)+\\
-12u\left[k\right]\left(-k^{2}+\left(k^{2}-t^{2}\right)u\left[k-t\right]+t^{2}u\left[-t\right]\right)\end{matrix}\\
t<0 & 0\end{cases}\\
 & = & \begin{cases}
12t^{2} & 0<t\leq6\\
4\left(72+t^{2}\right) & t>6\\
0 & \mbox{Else}\end{cases}\end{eqnarray*}

\end_inset


\end_layout

