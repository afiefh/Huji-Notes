\end_layout

\begin_layout Plain Layout
\begin_inset Caption

\begin_layout Plain Layout
חיבור מקבילי של רכיבים
\end_layout

\end_inset


\end_layout

\begin_layout Plain Layout

\end_layout

\end_inset


\end_layout

\begin_layout Subsection
נגדים
\end_layout

\begin_layout Standard
\begin_inset Formula \begin{eqnarray*}
i_{n} & = & \frac{V_{n}}{R_{n}}=\frac{V}{R_{n}}\\
i & = & \sum_{n}i_{n}\\
 & = & \sum_{n}\frac{V_{n}}{R_{n}}\\
 & = & V\sum_{n}\frac{1}{R_{n}}\\
\frac{V}{R} & = & V\sum\frac{1}{R_{n}}\\
\frac{1}{R} & = & \sum\frac{1}{R_{n}}\end{eqnarray*}

\end_inset


\end_layout

\begin_layout Standard
מקרה פרטי:
\end_layout

\begin_layout Standard
\begin_inset Formula \begin{eqnarray*}
\frac{1}{R} & = & \frac{1}{R_{1}}+\frac{1}{R_{2}}\\
\frac{1}{R} & = & \frac{R_{2}+R_{1}}{R_{1}R_{2}}\\
R & = & \frac{R_{1}R_{2}}{R_{1}+R_{2}}\end{eqnarray*}

\end_inset


\end_layout

\begin_layout Subsection
קבלים
\end_layout

\begin_layout Standard
\begin_inset Formula \begin{eqnarray*}
i_{n} & = & c_{n}\frac{\partial v_{n}}{\partial t}=c_{n}\frac{\partial V}{\partial t}\\
i & = & \sum_{n}c_{n}\frac{\partial V_{n}}{\partial t}\\
 & = & \frac{\partial V}{\partial t}\sum_{n}c_{n}\\
C & = & \sum_{n}c_{n}\end{eqnarray*}

\end_inset


\end_layout

\begin_layout Subsection
סלילים
\end_layout

\begin_layout Standard
\begin_inset Formula \begin{eqnarray*}
i_{n} & = & i_{n}\left(0\right)+\frac{1}{L_{n}}\sum_{0}^{t}v_{n}\left(t^{\prime}\right)dt^{\prime}\\
i & = & \sum_{n}i_{n}\\
 & = & \sum_{n}i_{n}\left(0\right)+\sum_{n}\frac{1}{L_{n}}\int_{0}^{t}V_{n}\left(t\right)dt\\
 & = & i\left(0\right)+\left[\int_{0}^{t}v\left(t^{\prime}\right)dt^{\prime}\right]\sum_{n}\frac{1}{L_{n}}\\
\frac{1}{L} & = & \sum_{n}\frac{1}{L_{n}}\end{eqnarray*}

\end_inset


\end_layout

