\begin_inset Caption

\begin_layout Plain Layout
דוגמה למעגל
\end_layout

\end_inset


\end_layout

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula \begin{eqnarray*}
L & = & 2H\\
C & = & 3F\\
R_{1} & = & 2\Omega\\
E_{2} & = & 4\Omega\\
V_{c}\left(0^{-}\right) & = & 8V\\
i_{c}\left(0^{-}\right) & = & 3A\end{eqnarray*}

\end_inset


\end_layout

\begin_layout Standard
צריכים להעביר המעגל למקור זרם אזי:
\end_layout

\begin_layout Standard
\begin_inset Float figure
wide false
sideways false
status open

\begin_layout Plain Layout
\begin_inset Graphics
	filename electricEngineering/c12d3.svg
	lyxscale 30
	width 70text%

\end_inset


\end_layout

\begin_layout Plain Layout
\begin_inset Caption

\begin_layout Plain Layout
זרם
\end_layout

\end_inset


\end_layout

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula \begin{eqnarray*}
G\cdot V & = & I\\
\begin{pmatrix}\frac{1}{R_{1}}+\frac{1}{LS} & -\frac{1}{LS}\\
-\frac{1}{LS} & \frac{1}{R_{2}}+cs+\frac{1}{LS}\end{pmatrix}\cdot\begin{pmatrix}v_{1}\left(s\right)\\
v_{2}\left(s\right)\end{pmatrix} & = & \begin{pmatrix}\frac{V_{s}\left(s\right)}{R_{1}}\\
0\end{pmatrix}\\
V_{c} & = & V_{2}\\
 & = & \frac{\nicefrac{V_{S}}{R_{1}SL}}{\left(\frac{1}{R_{1}}+\frac{1}{sL}\right)\left(\frac{1}{R_{2}}+\frac{1}{Ls}+sC\right)-\frac{1}{s^{2}L^{2}}}\end{eqnarray*}

\end_inset


\end_layout

\begin_layout Standard
את פותרים:
\end_layout

\begin_layout Standard
\begin_inset Formula \begin{eqnarray*}
V_{c}\left(s\right) & = & \frac{V_{s}\left(s\right)}{\frac{sL}{E_{2}}+1+\frac{R_{1}}{R_{2}}+\frac{R_{1}}{Ls}+s^{2}CL+SCR-\frac{R_{1}}{Ls}}\\
 & = & \frac{\nicefrac{V_{s}\left(s\right)}{CL}}{s^{2}+\left(\frac{1}{R_{2}C}+\frac{R_{1}}{L}\right)s+\frac{1}{LC}\left(1+\frac{R_{1}}{R_{2}}\right)}\\
 & = & s^{2}+\left(\frac{1}{R_{2}C}+\frac{R_{1}}{L}\right)s+\frac{1}{LC}\left(1+\frac{R_{1}}{R_{2}}\right)V_{c}\left(s\right)=\frac{1}{LC}V_{s}\left(s\right)\end{eqnarray*}

\end_inset


\end_layout

\begin_layout Standard
אזי
\end_layout

\begin_layout Standard
\begin_inset Formula \[
V_{c}^{\prime\prime}\left(t\right)+\left(\frac{1}{R_{2}C}+\frac{R_{1}}{L}\right)V_{c}^{\prime}\left(t\right)+\frac{1}{LC}\left(1+\frac{R_{1}}{R_{2}}\right)V_{c}\left(t\right)=\frac{1}{LC}V_{s}\left(t\right)\]

\end_inset


\end_layout

\begin_layout Standard
פותרים:
\end_layout

\begin_layout Standard
\begin_inset Formula \begin{eqnarray*}
V_{c}^{\prime\prime}+\frac{13}{12}V_{c}^{\prime}+\frac{1}{4}V_{C} & = & \frac{1}{6}V_{s}\\
\omega_{0} & = & \frac{1}{2}\\
2\alpha & = & \frac{13}{12}\\
Q & = & \frac{\omega_{0}}{2\alpha}=\frac{6}{3}<\frac{1}{2}\end{eqnarray*}

\end_inset


\end_layout

\begin_layout Standard
אנחנו בתחום של ריסון יתר.
\end_layout

\begin_layout Standard
מציאת תנאי ההתחלה
\end_layout

\begin_layout Standard
\begin_inset Formula \begin{eqnarray*}
I_{L}\left(0^{-}\right) & = & \frac{V_{c}\left(0^{-}\right)}{R_{2}}+CV_{C}^{\prime}\left(0^{-}\right)\\
V_{c}^{\prime}\left(0^{-}\right) & = & \frac{i_{L}\left(0^{-}\right)-\nicefrac{V_{c}}{R_{2}}}{C}\\
V_{c}^{\prime}\left(0^{-}\right) & = & \frac{1}{3}\end{eqnarray*}

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula $ZIR$
\end_inset

:
\end_layout

\begin_layout Standard
\begin_inset Formula \begin{eqnarray*}
P\left(s\right) & = & s^{2}+\frac{13}{12}s+\frac{1}{4}=0\\
S_{1,2} & = & -\frac{1}{3},-\frac{3}{4}\\
V_{c}^{ZIR} & = & Ae^{-\frac{1}{3}t}+Be^{-\frac{3}{4}t}\\
A+B & = & 8\\
\frac{1}{3}A-\frac{3}{4}B & = & \frac{1}{3}\\
\Rightarrow V_{C}^{ZIR} & = & 15.2e^{-\frac{t}{3}}-7.2e^{-\frac{-t}{4}}\end{eqnarray*}

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula $ZSR$
\end_inset

:
\end_layout

\begin_layout Standard
\begin_inset Formula \begin{eqnarray*}
V_{c}^{\prime\prime}+\frac{13}{12}V_{c}^{\prime}+\frac{1}{4}V_{c} & = & \overbrace{\frac{1}{6}}^{\frac{1}{LC}V_{s}}\left(1+2t\right)u\left(t\right)\\
V_{p} & = & a+bt\\
V_{C}^{ZSR} & = & \left(V_{p}+V_{h}\right)u\left(t\right)\\
 & = & \left[a+bt+Ae^{-\frac{t}{3}}+Be^{-\frac{3t}{4}}\right]u\left(t\right)\end{eqnarray*}

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula \begin{eqnarray*}
\frac{13}{12}b+\frac{1}{4}a+\frac{1}{4}bt & = & \frac{1}{c}\left(1+2t\right)\\
\Rightarrow a & = & -5\frac{1}{9}\\
b & = & \frac{4}{3}\\
V_{c}^{ZSR} & = & \left(-5\frac{1}{9}+\frac{3}{4}t+Ae^{-\frac{t}{3}}+Be^{-\frac{3t}{4}}\right)u\left(t\right)\\
V_{C}\left(0^{+}\right):\,-5\frac{1}{9}+A+B & = & 0\\
V_{C}^{\prime}\left(0^{+}\right):\,\frac{4}{3}-\frac{1}{3}A-\frac{3}{4}B & = & 0\\
A & = & 6\\
B & = & -\frac{8}{9}\end{eqnarray*}

\end_inset


\end_layout

\begin_layout Standard
אזי
\end_layout

\begin_layout Standard
\begin_inset Formula \begin{eqnarray*}
V_{c}^{ZSR}\left(t\right) & = & \left[-5\frac{1}{9}+\frac{4}{3}t+6e^{-\frac{t}{3}}-\frac{8}{9}e^{-\frac{3t}{4}}\right]u\left(t\right)\\
V_{c}\left(t\right) & = & V_{C}^{ZIR}+V_{C}^{ZSR}\end{eqnarray*}

\end_inset


\end_layout

\begin_layout Section
פתרון מעגל מסדר שלישי
\end_layout

\begin_layout Standard
\begin_inset Formula \begin{eqnarray*}
V_{c}^{\prime\prime\prime}+4V_{c}^{\prime\prime\prime}+6V_{c}^{\prime\prime}+4V_{C} & = & 0\\
s^{3}+4^{2}+6s+4 & = & 0\\
S_{1,2} & = & -2\\
S_{2,3} & = & -1\pm j\\
V_{c} & = & Ae^{-2t}+e^{-t}\left[B\sin\left(t\right)+C\cos\left(t\right)\right]\end{eqnarray*}

\end_inset


\end_layout

