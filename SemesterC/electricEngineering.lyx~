\end_layout

\begin_layout Standard
\begin_inset Formula \begin{eqnarray*}
I_{R} & = & \frac{V_{R}}{R}\\
I_{L} & = & i_{L}\left(0\right)+\frac{1}{L}\int_{0}^{t}V_{L}\left(t^{\prime}\right)dt^{\prime}\\
I_{c} & = & C\cdot V_{c}^{\prime}\end{eqnarray*}

\end_inset


\end_layout

\begin_layout Standard
מציבים:
\end_layout

\begin_layout Standard
\begin_inset Formula \begin{eqnarray*}
CV_{C}^{\prime}+i_{L}\left(0\right)+\frac{1}{L}\int_{0}^{t}Vdt+\frac{V}{R} & = & I_{s}\\
V^{\prime\prime}+\frac{1}{RC}V^{\prime}+\frac{1}{LC}V & = & \frac{1}{C}i_{s}^{\prime}\end{eqnarray*}

\end_inset


\end_layout

