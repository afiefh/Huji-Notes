\begin_layout Standard
\begin_inset Formula \begin{eqnarray*}
\hat{V}_{1} & = & 10\angle0^{\circ}\\
\omega & = & 10\frac{\mbox{rad}}{\mbox{sec}}\\
M & = & 9H\\
L_{1} & = & 1H\\
L_{2} & = & 100H\\
j\omega L_{1} & = & 10j\\
j\omega L_{2} & = & 100j\\
j\omega M & = & 90j\end{eqnarray*}

\end_inset


\end_layout

\begin_layout Standard
לפי 
\begin_inset Formula $KVL$
\end_inset

:
\end_layout

\begin_layout Standard
\begin_inset Formula \begin{eqnarray*}
\hat{V_{1}} & = & 10\angle0=\hat{V}_{L_{1}}+1\hat{I}_{1}=j\omega L_{1}\hat{I}_{1}-j\omega M\hat{I}_{2}+1\hat{I}_{1}\\
10\angle0 & = & 10j\hat{I}_{1}-90\hat{I}_{2}+\hat{I}_{1}\end{eqnarray*}

\end_inset


\end_layout

\begin_layout Standard
צד ימני:
\end_layout

\begin_layout Standard
