\begin_layout Standard
\begin_inset Formula \begin{eqnarray*}
V_{1} & = & L_{1}\frac{dI_{1}}{dt}+M\frac{dI_{2}}{dt}\\
V_{2} & = & L_{2}\frac{dI_{2}}{dt}+M\frac{dI_{1}}{dt}\\
\kappa & = & \frac{\left|M\right|}{\sqrt{L_{1}L_{_{2}}}}\end{eqnarray*}

\end_inset


\end_layout

\begin_layout Standard
כאשר 
\begin_inset Formula $\kappa$
\end_inset

 הוא מקדם הצימוד והוא מציין כמה שטף עובר מסליל אחד לשני
\end_layout

\begin_layout Standard
במצב סינוסי עמיד:
\end_layout

\begin_layout Standard
\begin_inset Formula \begin{eqnarray*}
\hat{V}_{1} & = & j\omega L_{1}\hat{I}_{1}+j\omega M\hat{I}_{2}\\
\hat{V}_{2} & = & j\omega L_{2}\hat{I}_{2}+j\omega M\hat{I}_{1}\end{eqnarray*}

\end_inset


\end_layout

