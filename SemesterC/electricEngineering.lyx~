\begin_layout Plain Layout
\begin_inset Caption

\begin_layout Plain Layout
חיבור של שתי רכיבים בטור
\end_layout

\end_inset


\end_layout

\begin_layout Plain Layout

\end_layout

\end_inset


\end_layout

\begin_layout Subsection
נגדים
\end_layout

\begin_layout Standard
\begin_inset Formula \begin{eqnarray*}
i & = & i_{1}=i_{2}\\
v & = & v_{1}+v_{2}\\
R & = & \frac{v}{i}\\
 & = & \frac{v_{1}}{i}+\frac{v_{2}}{i}\\
 & = & \frac{v_{1}}{i_{1}}+\frac{v_{2}}{i_{2}}\\
 & = & R\\
R & = & \sum_{n=1}^{N}R_{n}\end{eqnarray*}

\end_inset


\end_layout

\begin_layout Subsection
קבלים
\end_layout

\begin_layout Standard
\begin_inset Formula \begin{eqnarray*}
V_{n}\left(t\right) & = & V_{0}\left(0\right)+\frac{1}{c_{n}}\int_{0}^{t}i_{n}\left(t^{\prime}\right)dt^{\prime}\\
V\left(t\right) & = & \sum_{n}V_{n}\left(t\right)\\
 & = & \sum_{n}V_{n}\left(0\right)+\sum_{n}\frac{1}{c_{n}}\int_{0}^{t}i_{n}\left(t^{\prime}\right)dt^{\prime}\\
 & = & V\left(0\right)+\left[\int_{0}^{t}i\left(t^{\prime}\right)dt\right]\sum_{n}\frac{1}{c_{n}}\\
\frac{1}{c} & = & \sum_{n}\frac{1}{c_{n}}\\
V\left(0\right) & = & \sum V_{n}\left(0\right)\end{eqnarray*}

\end_inset


\end_layout

\begin_layout Subsection
סלילים
\end_layout

\begin_layout Standard
\begin_inset Formula \begin{eqnarray*}
V_{n} & = & L_{n}\frac{\partial i_{n}}{\partial t}\\
V & = & \sum_{n}V_{n}-\sum_{n}L_{n}\frac{\partial i_{n}}{\partial t}\\
 & = & \frac{\partial i}{\partial t}\sum_{n}L_{n}\\
L & = & \sum_{n}L_{n}\end{eqnarray*}

\end_inset


\end_layout

