\begin_layout Standard
\begin_inset Formula \begin{eqnarray*}
Li_{L}^{\prime} & = & i_{R}R\\
i_{s} & = & i_{R}+i_{L}\\
i_{L}\left(t\right)+\frac{L}{R}i_{L}^{\prime} & = & I_{s}u\left(t\right)\\
i_{L}^{\prime}+\frac{R}{L}i_{L} & = & \frac{R}{L}I_{s}u\left(t\right)\\
i_{L}\left(t\right) & = & I_{s}\left(1-e^{-\frac{R}{L}t}\right)u\left(t\right)\\
v_{L}\left(t\right) & = & Li_{L}^{\prime}\\
 & = & Re^{-\frac{R}{L}t}I_{s}u\left(t\right)\end{eqnarray*}

