\begin_layout Standard
\begin_inset Formula \begin{eqnarray*}
\int_{0^{-}}^{0^{+}}v^{\prime\prime}+16\int_{0^{-}}^{0^{+}}+100\int_{0^{-}}^{0^{+}}v & = & 2\int_{0^{-}}^{0^{+}}\delta\left(t\right)\\
\left[v^{\prime}\left(0^{+}\right)-v^{\prime}\left(0^{-}\right)\right]+16\left[v\left(0^{+}\right)-v\left(0^{-}\right)\right]+0 & = & 2\\
v^{\prime}\left(0^{+}\right)+16v\left(0^{+}\right) & = & 2\end{eqnarray*}

\end_inset


\end_layout

\begin_layout Standard
