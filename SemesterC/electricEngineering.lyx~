\begin_layout Standard
\begin_inset Formula \begin{eqnarray*}
V_{C_{2}}+V_{R}-V_{C_{1}} & = & 0\\
\frac{1}{c_{2}}\int_{-\infty}^{t}idt+iR-\frac{i}{c_{1}}\int_{-\infty}^{t}-idt & = & 0\\
\left(\frac{1}{c_{1}}+\frac{1}{c_{2}}\right)\int idt+iR & = & 0\\
\frac{\partial i}{\partial t}+\frac{1}{R}\left(\frac{1}{c_{1}}+\frac{1}{c_{2}}\right)i & = & 0\\
\frac{\partial i}{\partial t}+\frac{1}{\tau}i & = & 0\end{eqnarray*}

\end_inset


\end_layout

\begin_layout Standard
כאשר 
\begin_inset Formula $\tau=\frac{RC}{2}$
\end_inset

.
\end_layout

\begin_layout Standard
מציאת תנאי התחלה:
\end_layout

\begin_layout Standard
\begin_inset Formula \begin{eqnarray*}
V_{C_{2}}+R\cdot i\left(0^{+}\right)-V_{c_{1}}\left(0^{+}\right) & = & 0\\
i\left(0^{+}\right) & = & \frac{V_{C_{1}}\left(0^{+}\right)-V_{C_{2}}\left(0^{+}\right)}{R}\\
 & = & \frac{V_{C_{1}}\left(0^{-}\right)-V_{C_{2}}\left(0^{-}\right)}{R}\\
 & = & \frac{V_{0}-0}{R}\\
 & = & \frac{V_{0}}{R}\end{eqnarray*}

\end_inset


\end_layout

\begin_layout Standard
זה נכון בגלל רציפות מתח בקבל
\end_layout

\begin_layout Standard
\begin_inset Formula \begin{eqnarray*}
i\left(t\right) & = & \frac{V_{0}}{R}e^{-\frac{t}{\tau}}\\
 & = & \frac{V_{0}}{R}e^{-\frac{2t}{RC}}\end{eqnarray*}

\end_inset


\end_layout

\begin_layout Standard
מתחי קבלים:
\end_layout

\begin_layout Standard
\begin_inset Formula \begin{eqnarray*}
V_{c_{1}}\left(t\right) & = & V_{0}+\frac{1}{c_{1}}\int-i\left(t\right)dt\\
 & = & V_{0}+\frac{V_{0}}{RC}\int_{0}^{t}e^{-\frac{2t}{RC}}dt\\
 & = & \frac{V_{0}}{2}+\frac{V_{0}}{2}e^{-\frac{2t}{RC}}\end{eqnarray*}

\end_inset


\end_layout

\begin_layout Paragraph
אנרגיה:
\end_layout

\begin_layout Standard
\begin_inset Formula \begin{eqnarray*}
P_{R}\left(t\right) & = & i^{2}\left(t\right)\cdot R=\frac{V_{0}^{2}}{R}e^{-\frac{4t}{RC}}\\
E_{R}\left(t\right) & = & \int_{0}^{t}P\left(t\right)dt\\
 & = & \frac{CV_{0}^{2}}{4}\left(1-e^{-\frac{4t}{RC}}\right)\\
E_{R}\left(\infty\right) & = & \frac{CV_{0}^{2}}{4}\\
E_{C_{1}}\left(\infty\right) & = & E_{C_{2}}\left(\infty\right)\\
 & = & \frac{1}{2}C\left(\frac{V_{0}}{2}\right)^{2}\\
 & = & \frac{1}{8}CV_{0}^{2}\end{eqnarray*}

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula \[
E_{tot}=2\frac{1}{8}CV_{0}^{2}+\frac{1}{4}CV_{0}^{2}=\frac{1}{2}CV_{0}^{2}\]

\end_inset


\end_layout

