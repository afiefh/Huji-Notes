\begin_layout Standard
\begin_inset Formula \begin{eqnarray*}
Mz_{cm} & = & \int_{V}z\rho\partial V\\
 & = & \rho\iiint z\partial x\partial x\partial z\\
 & = & \rho\int_{0}^{h}\int_{0}^{\frac{h}{\tan\left(\alpha\right)}-\frac{z}{\tan\left(\alpha\right)}}\int_{0}^{2\pi}zr\partial\theta\partial r\partial z\\
 & = & 2\pi\rho\int_{0}^{h}\int_{0}^{\frac{h}{\tan\left(\alpha\right)}-\frac{z}{\tan\left(\alpha\right)}}zr\partial r\partial z\\
 & = & 2\pi\rho\int_{0}^{h}\left[\frac{1}{2}z\cdot r^{2}\right]_{0}^{\frac{h}{\tan\left(\alpha\right)}-\frac{z}{\tan\left(\alpha\right)}}\partial z\\
 & = & \pi\rho\int_{0}^{h}z\cdot\left(\frac{h}{\tan\left(\alpha\right)}-\frac{z}{\tan\left(\alpha\right)}\right)^{2}\partial z\\
 & = & \pi\rho\int_{0}^{h}z\cdot\left(\frac{h^{2}}{\tan^{2}\left(\alpha\right)}-2\frac{hz}{\tan^{2}\left(\alpha\right)}+\frac{z^{2}}{\tan^{2}\left(\alpha\right)}\right)\partial z\\
 & = & \pi\rho\int_{0}^{h}\left(\frac{zh^{2}}{\tan^{2}\left(\alpha\right)}-2\frac{hz^{2}}{\tan^{2}\left(\alpha\right)}+\frac{z^{3}}{\tan^{2}\left(\alpha\right)}\right)\partial z\\
 & = & \pi\rho\left[\frac{z^{2}h^{2}}{2\tan^{2}\left(\alpha\right)}-2\frac{hz^{3}}{3\tan^{2}\left(\alpha\right)}+\frac{z^{4}}{4\tan^{2}\left(\alpha\right)}\right]_{0}^{h}\\
 & = & \pi\rho\left(\frac{\frac{h^{4}}{2}}{\tan^{2}\left(\alpha\right)}-\frac{\frac{2h^{4}}{3}}{\tan^{2}\left(\alpha\right)}+\frac{\frac{h^{4}}{4}}{\tan^{2}\left(\alpha\right)}\right)\\
 & = & \pi\rho\left(\frac{h^{4}\frac{6-8+3}{12}}{\tan^{2}\left(\alpha\right)}\right)\\
 & = & \pi\rho\left(\frac{h^{4}}{12\tan^{2}\left(\alpha\right)}\right)\end{eqnarray*}

