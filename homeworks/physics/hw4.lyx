\begin_layout Standard
\begin_inset Formula \begin{eqnarray*}
M & = & \rho\iiint\rho\partial V\\
 & = & \rho\iiint\partial x\partial x\partial z\\
 & = & \rho\int_{0}^{h}\int_{0}^{\frac{h}{\tan\left(\alpha\right)}-\frac{z}{\tan\left(\alpha\right)}}\int_{0}^{2\pi}r\partial\theta\partial r\partial z\\
 & = & 2\pi\rho\int_{0}^{h}\int_{0}^{\frac{h}{\tan\left(\alpha\right)}-\frac{z}{\tan\left(\alpha\right)}}r\partial r\partial z\\
 & = & 2\pi\rho\int_{0}^{h}\left[\frac{r^{2}}{2}\right]_{0}^{\frac{h}{\tan\left(\alpha\right)}-\frac{z}{\tan\left(\alpha\right)}}\partial z\\
 & = & \pi\rho\int_{0}^{h}\left(\frac{h}{\tan\left(\alpha\right)}-\frac{z}{\tan\left(\alpha\right)}\right)^{2}\partial z\\
 & = & \pi\rho\int_{0}^{h}\left(\frac{h^{2}}{\tan^{2}\left(\alpha\right)}-2\frac{hz}{\tan^{2}\left(\alpha\right)}+\frac{z^{2}}{\tan^{2}\left(\alpha\right)}\right)\partial z\\
 & = & \pi\rho\left[\frac{h^{2}z}{\tan^{2}\left(\alpha\right)}-\frac{hz^{2}}{\tan^{2}\left(\alpha\right)}+\frac{z^{3}}{3\tan^{2}\left(\alpha\right)}\right]_{0}^{h}\\
 & = & \pi\rho\left(\frac{h^{3}}{\tan^{2}\left(\alpha\right)}-\frac{h^{3}}{\tan^{2}\left(\alpha\right)}+\frac{h^{3}}{3\tan^{2}\left(\alpha\right)}\right)\\
 & = & \pi\rho\left(\frac{\frac{h^{3}}{3}}{\tan^{2}\left(\alpha\right)}\right)\end{eqnarray*}

\end_inset


