x=-1.1:0.05:10;
subplot(2,3,1);
plot(x,log(x+1),"-",x,x,".");
axis([-0.9,2,-2.5,1]);
subplot(2,3,2);
plot(x,log(x+1),"-",x,x-x.^2./2,".");
axis([-0.9,2,-2.5,1]);
subplot(2,3,3);
plot(x,log(x+1),"-",x,x-x.^2./2.+x.^3./3,".");
