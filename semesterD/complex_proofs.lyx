f_{1}^{\prime}\left(z\right) & = & \frac{u\left(z+t\right)+iv\left(z+t\right)-u\left(z\right)-iv\left(z\right)}{t}\\
 & = & \frac{u\left(z+t\right)-u\left(z\right)}{t}+i\frac{v\left(z+t\right)-v\left(z\right)}{t}\\
 & = & \frac{\partial u\left(z\right)}{\partial x}+i\frac{\partial v\left(z\right)}{\partial x}\\
f_{2}^{\prime}\left(z\right) & = & \frac{u\left(z+it\right)+iv\left(z+it\right)-u\left(z\right)-iv\left(z\right)}{it}\\
 & = & \frac{u\left(x+i\left(y+t\right)\right)+iv\left(x+i\left(y+t\right)\right)-u\left(z\right)-iv\left(z\right)}{it}\\
 & = & \frac{u\left(x+i\left(y+t\right)\right)-u\left(z\right)}{it}+i\frac{v\left(x+i\left(y+t\right)\right)-v\left(z\right)}{it}\\
 & = & -i\frac{u\left(x+i\left(y+t\right)\right)-u\left(z\right)}{t}+\frac{v\left(x+i\left(y+t\right)\right)-v\left(z\right)}{t}\\
 & = & -i\frac{\partial u\left(z\right)}{\partial y}+\frac{\partial v\left(z\right)}{\partial y}\end{eqnarray*}

